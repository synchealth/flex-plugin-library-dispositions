{
  "name": "aplibc-dispositions",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "test": "jest",
    "test:coverage": "npm run test -- --coverage",
    "lint": "eslint --ext ts --ext tsx ui-src/src/",
    "lint:fix": "npm run lint -- --fix",
    "makeassetsprivate": "cd ui-src/build && sh ../../scripts/makeAssetsPrivate.sh",
    "predeploy": "twilio plugins:install @twilio-labs/plugin-serverless",
    "deploy": "twilio serverless:deploy",
    "build": "cd ui-src && twilio flex:plugins:build && cd .. && npm run makeassetsprivate",
    "prepare": "husky install"
  },
  "dependencies": {
    "@twilio-labs/plugin-serverless": "^3.1.3",
    "@twilio-labs/runtime-helpers": "^0.1.2",
    "@twilio-labs/serverless-api": "^5.5.0",
    "@twilio-labs/serverless-runtime-types": "^2.2.3",
    "lodash": "^4.17.21",
    "twilio": "^4.0.0",
    "twilio-flex-token-validator": "1.5.8"
  },
  "devDependencies": {
    "@babel/preset-env": "7.20.2",
    "@babel/preset-modules": "0.1.5",
    "@babel/preset-react": "7.18.6",
    "@babel/preset-typescript": "^7.12.1",
    "@emotion/jest": "^11.10.5",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "12.1.5",
    "@twilio/flex-ui": "2.1.1",
    "@types/jest": "^26.0.24",
    "@types/lodash": "4.14.182",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.0",
    "jest": "^26.0.20",
    "jest-fetch-mock": "3.0.3",
    "jest-junit": "12.0.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "twilio-cli": "^5.5.0",
    "twilio-run": "3.4.2"
  },
  "engines": {
    "node": "16"
  }
}
